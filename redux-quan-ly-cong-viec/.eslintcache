v className=\"form-group\">\r\n            <label>Tên</label>\r\n            <input\r\n            type=\"text\"\r\n            name='name'\r\n            value={formValue.name}\r\n            onChange={onChange}\r\n            />\r\n          </div>\r\n\r\n          <div className=\"form-group\">\r\n            <label>Trạng Thái:</label>\r\n            <select\r\n            name='status'\r\n            value={formValue.status}\r\n            onChange={onChange} >\r\n              <option value={true}>Kích Hoạt</option>\r\n              <option value={false}>Ẩn</option>\r\n            </select>\r\n          </div>\r\n\r\n          <div className=\"form-group\">\r\n          <button type='submit' className='btn btn-add'>Them Cong viec</button>\r\n          </div>\r\n        </form>\r\n\r\n        <div className=\"taskForm-footer\">\r\n        <button onClick={onClear} className='btn btn-cancel'>Hủy Bỏ</button>\r\n        </div>\r\n      </div>\r\n\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default TaskForm;","C:\\Users\\quang\\OneDrive\\Máy tính\\reactjs-projects\\quan-ly-cong-viec\\src\\components\\Control\\Sort.jsx",[],"C:\\Users\\quang\\OneDrive\\Máy tính\\reactjs-projects\\quan-ly-cong-viec\\src\\components\\Control\\Search.jsx",[],"C:\\Users\\quang\\OneDrive\\Máy tính\\reactjs-projects\\quan-ly-cong-viec\\src\\components\\Tasks\\TaskItem.jsx",[],"C:\\Users\\quang\\OneDrive\\Máy tính\\reactjs-projects\\quan-ly-cong-viec\\src\\components\\Tasks\\TaskList.jsx",[],"C:\\Users\\quang\\OneDrive\\Máy tính\\reactjs-projects\\redux-quan-ly-cong-viec\\src\\index.js",[],"C:\\Users\\quang\\OneDrive\\Máy tính\\reactjs-projects\\redux-quan-ly-cong-viec\\src\\App.js",["86","87","88","89"],"C:\\Users\\quang\\OneDrive\\Máy tính\\reactjs-projects\\redux-quan-ly-cong-viec\\src\\components\\TaskForm\\TaskForm.jsx",["90"],"C:\\Users\\quang\\OneDrive\\Máy tính\\reactjs-projects\\redux-quan-ly-cong-viec\\src\\components\\Control\\Search.jsx",[],["91","92"],"C:\\Users\\quang\\OneDrive\\M